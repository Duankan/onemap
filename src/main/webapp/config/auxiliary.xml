        <Type>Tile</Type>
        <TypeEx>Image,Vector</TypeEx>
        <Args>{subdomains:["0", "1", "2", "3", "4", "5", "6", "7"]}</Args>
        <MaxZoom>20</MaxZoom>
        <MinZoom>1</MinZoom>
        <Visible>true</Visible>
        <Order>3</Order>
      </BaseLayers>
      <BaseLayers>
        <ID>tdt_image_vector_name</ID>
        <Name>天地图_影像_矢量_地名标注</Name>
        <Url><![CDATA[http://t{s}.tianditu.cn/cia_w/wmts?service=wmts&request=GetTile&version=1.0.0&LAYER=cia&tileMatrixSet=w&TileMatrix={z}&TileRow={y}&TileCol={x}&style=default&format=tiles]]></Url>
