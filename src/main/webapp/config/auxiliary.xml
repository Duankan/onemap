        <Type>Tile</Type>
        <TypeEx>Image</TypeEx>
        <Args>{subdomains:["0", "1", "2", "3", "4", "5", "6", "7"]}</Args>
        <MaxZoom>20</MaxZoom>
        <MinZoom>1</MinZoom>
        <Visible>true</Visible>
        <Order>1</Order>
      </BaseLayers>
      <BaseLayers>
        <ID>tdt_vector</ID>
